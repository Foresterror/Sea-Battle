<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Rectangle?>
<AnchorPane prefHeight="132.0" prefWidth="239.0" xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/2.2" fx:controller="sampleFX.Controller">
    <children>
        <Rectangle fx:id="ship1" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="22.0" layoutX="33.0"
                   layoutY="21.0" onDragDetected="#shipDragDetected" onDragDone="#shipDragDone" stroke="BLACK"
                   strokeType="INSIDE" width="28.0"/>
        <GridPane fx:id="gridPane" gridLinesVisible="true" layoutX="145.0" layoutY="21.0"
                  onDragDropped="#gridPaneDragDropped" onDragEntered="#gridPaneDragEntered"
                  onDragExited="#gridPaneDragExited" onDragOver="#gridPaneDragOver"
                  onMouseClicked="#gridPaneMouseClicked" onMouseMoved="#gridPaneMouseMoved" prefHeight="48.0"
                  prefWidth="56.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="33.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="33.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="33.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" percentHeight="33.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="33.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="33.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
        <Rectangle id="ship1" fx:id="ship2" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="22.0"
                   layoutX="33.0" layoutY="47.0" onDragDetected="#shipDragDetected" onDragDone="#shipDragDone"
                   stroke="BLACK" strokeType="INSIDE" width="28.0"/>
        <Label fx:id="label" labelFor="$gridPane" layoutX="159.0" layoutY="81.0" text="Label" textAlignment="CENTER"/>
    </children>
</AnchorPane>
